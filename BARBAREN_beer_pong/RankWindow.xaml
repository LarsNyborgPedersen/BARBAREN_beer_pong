<Window x:Class="BARBAREN_beer_pong.RankWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BARBAREN_beer_pong"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Title="RankWindow" Height="450" Width="800">

    

    <ScrollViewer HorizontalScrollBarVisibility="Auto" >
        <StackPanel x:Name="myStackPanel">
            <Label Content="Bedste teams" HorizontalAlignment="Center" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" FontSize="100"/>
            <WrapPanel x:Name="wrapPanel" Height="auto"/>


        </StackPanel>
    </ScrollViewer>
</Window>
